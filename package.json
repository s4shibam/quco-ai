{
  "name": "quco",
  "version": "1.0.0",
  "description": "Turn natural language into shell commands using AI",
  "bin": {
    "quco": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "scripts/postinstall.js"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "biome check --write ./src",
    "format": "biome format --write ./src",
    "check": "biome check ./src",
    "prepublishOnly": "npm run lint && npm run build",
    "postinstall": "node scripts/postinstall.js || true"
  },
  "keywords": [
    "cli",
    "ai",
    "terminal",
    "shell",
    "command",
    "llm",
    "openai",
    "anthropic",
    "gemini"
  ],
  "author": "s4shibam",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.40",
    "@ai-sdk/google": "^2.0.26",
    "@ai-sdk/openai": "^2.0.59",
    "@inquirer/prompts": "^7.9.0",
    "ai": "^5.0.86",
    "chalk": "^4.1.2",
    "commander": "^14.0.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@types/node": "^24.9.2",
    "typescript": "^5.9.3"
  }
}
